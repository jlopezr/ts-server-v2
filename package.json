{
  "name": "ts-mongoose",
  "version": "1.0.0",
  "description": "mongoose propagation in ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc --watch",
    "start": "node ./lib/index.js",
    "test": "jest --coverage --config jestconfig.json",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./src/",
    "clean": "shx rm -rf node_modules/ lib/ docs/ coverage/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlopezr/my-awesome-test.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Hello",
    "Greeter"
  ],
  "author": "jlopezr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jlopezr/my-awesome-test/issues"
  },
  "homepage": "https://github.com/jlopezr/my-awesome-test#readme",
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.5",
    "@types/lodash": "^4.14.123",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.3.24",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.7",
    "coveralls": "^3.0.2",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "shx": "^0.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.2",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "typegoose": "^5.6.0",
    "winston": "^3.2.1"
  }
}
